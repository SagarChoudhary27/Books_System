<% layout("/layouts/boilerplate") %>
 
	<h3 class="mt-3 mb-3">All Books</h3>
	<br>
	<hr>

	<ul class="list-group"></ul>
		<% for(let book of allBooks){ %>
			
				<li class="list-group-item">
					<a href="/books/<%=book._id%>" class="book-link">
						<b><%= book.title %></b><br/><%= book.description %>
					</a>
				</li>
				<hr>

		<% } %>
	</ul>